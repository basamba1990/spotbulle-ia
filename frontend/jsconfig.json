{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/app/*": ["src/app/*"],
      "@/lib/*": ["src/lib/*"],
      "@/context/*": ["src/context/*"],
      "@/styles/*": ["src/styles/*"]
